FUNCTION 3des_sb5 (in_1, in_2, in_3, in_4, in_5, in_6)
	RETURNS (out_1, out_2, out_3, out_4);

SUBDESIGN 3des_sbox5
(
	WE[5..0]	:input;
	WY[3..0]	:output;
)
variable 
	s5	:3des_sb5;
begin

	s5.in_1	=	WE[5];
	s5.in_2	=	WE[4];
	s5.in_3	=	WE[3];
	s5.in_4	=	WE[2];
	s5.in_5	=	WE[1];
	s5.in_6	=	WE[0];
	WY[3]	=	s5.out_1;
	WY[2]	=	s5.out_2;
	WY[1]	=	s5.out_3;
	WY[0]	=	s5.out_4;
end;


